	grd2xyz $tmp.sed.grd.tmp | outin $prj.INT -V0 | \
		awk -v dx=$dx -v dy=$dy '{if ($1=="1") {seds_in+=$4*dx*dy;}} END{printf("\nSediment_region_1: %.1f km3", seds_in);}'
