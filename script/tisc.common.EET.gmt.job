grdimage 	$tmp.eeth.grd.tmp -C$tmp.eet_cpt.tmp  -JX -R -K -O >> $ps
grdcontour	$tmp.eeth.grd.tmp -JX -R$Region -C2  -A4   -O -K >> $ps
grdcontour	$tmp.xyg.grd.tmp  -JX -R$Region -C50 -A100 -W1/255/0/0 -O -K >> $ps

